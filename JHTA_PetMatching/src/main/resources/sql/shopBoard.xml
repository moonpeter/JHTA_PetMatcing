<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ShopBoards">
	<insert id="insert" parameterType="ShopBoard">
		insert into shop_board
		values (#{shop_num}, #{shop_category}, #{shop_title}, #{shop_price}, #{shop_country_of_origin}, #{shop_brand},#{shop_thumnail}, #{shop_text_content})
	</insert>
</mapper>